{
  "version": 2,
  "routes": [
    {
      "src": "/css/(.*)",
      "dest": "css/$1"
    },
    {
      "src": "/js/(.*)",
      "dest": "js/$1"
    },
    {
      "src": "/public/(.*)", 
      "dest": "public/$1"
    },
    {
      "src": "/(.*\\.html)",
      "dest": "$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}