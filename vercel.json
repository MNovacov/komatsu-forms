{
  "version": 2,
  "cleanUrls": false,
  "trailingSlash": false,
  "routes": [
    {
      "src": "^/(?:\\.well-known(?:/.*)?|css/.*|js/.*|public/.*|.*\\.(?:[0-9a-f]{8}|css|js|json|html?|map|txt|xml|png|jpe?g|gif|svg|ico|webp|woff2?|eot|ttf|otf))$",
      "dest": "/$1"
    },
    {
      "src": "^(?!/?$)(/.*)?$",
      "dest": "/index.html"
    }
  ]
}